{
  "event_id": "{{ _id }}00000000",
  "culprit": "{{ _links.site.href }}",
  "logger": "launchdarkly",
  "platform": "other",
  "level": "info",
  "transaction": "{{ title.plainText }}",
  "server_name": "{{ baseURL }}",
  "fingerprint": [
    "{{ _links.site.href }}",
    "{{ kind }}",
    "{{ name }}",
    "{{ title.plainText }}"
  ],

  "tags": {
    "source": "LaunchDarkly",
    "launchdarkly_kind": "{{ kind }}",
    "launchdarkly_name": "{{ name }}",
    "launchdarkly_key": "{{key}}"
  },

  "message": {
    "formatted": "{{{ title.plainText }}}\n{{{ details.plainText }}}{{#if comment}}\n\n{{ comment }}{{/if}}"
  },
  "user": {
    "id": "launchdarkly/member/{{ member._id }}",
    "email": "{{ member.email }}",
    "Member Name": "{{ member.firstName }} {{ member.lastName  }}"
  }
}
