{
  "event_id": "{{ _id }}00000000",
  "culprit": "https://app.launchdarkly.com{{ _links.site.href }}",
  "logger": "launchdarkly.integration",
  "platform": "other",
  "level": "info",
  "transaction": "{{ title }}",
  "server_name": "app.launchdarkly.com",
  "fingerprint": [
    "{{ _links.site.href }}",
    "{{ kind }}",
    "{{ name }}",
    "{{ title }}"
  ],

  "tags": {
    "source": "launchdarkly",
    "launchdarkly_kind": "{{ kind }}",
    "launchdarkly_name": "{{ name }}"
  },

  "message": {
    "formatted": "{{ title }}\n{{ description }}\n\n{{ comment }}"
  },
  "user": {
    "id": "launchdarkly/member/{{ member._id }}",
    "email": "{{ member.email }}",
    "Member Name": "{{ member.firstName }} {{ member.lastName  }}"
  }
 
}