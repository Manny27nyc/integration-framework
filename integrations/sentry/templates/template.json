{
  "event_id": "{{ _id }}00000000",
  "culprit": "{{ _links.site.href }}",
  "logger": "launchdarkly.integration",
  "platform": "other",
  "level": "info",
  "transaction": "{{ titleObj.plainText }}",
  "server_name": "app.launchdarkly.com",
  "fingerprint": [
    "{{ _links.site.href }}",
    "{{ kind }}",
    "{{ name }}",
    "{{ titleObj.plainText }}"
  ],

  "tags": {
    "source": "launchdarkly",
    "launchdarkly_kind": "{{ kind }}",
    "launchdarkly_name": "{{ name }}"
  },

  "message": {
    "formatted": "{{ titleObj.markdown }}\n{{ details.markdown }}{{#if comment}}\n\n{{ comment }}{{/if}}"
  },
  "user": {
    "id": "launchdarkly/member/{{ member._id }}",
    "email": "{{ member.email }}",
    "Member Name": "{{ member.firstName }} {{ member.lastName  }}"
  }
}
